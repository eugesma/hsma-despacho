<div class="row">
  <div class="col-md-6">
    <p>
      <h5><strong>Nombre sector:</strong></h5>
      <%= @sector.sector_name %>
    </p>

    <p>
      <h5><strong>Descripción:</strong></h5>
      <%= @sector.description %>
    </p>
  </div>

  <div class="col-md-6">
    <p>
      <h5><strong>Fecha de creación:</strong></h5>
      <%= @sector.created_at.strftime("%d/%m/%y %H:%M") %>
    </p>

    <p>
      <h5><strong>Fecha de modificación:</strong></h5>
      <%= @sector.updated_at.strftime("%d/%m/%y %H:%M") %>
    </p>
  </div>
</div>
